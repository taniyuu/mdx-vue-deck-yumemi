xt-white font-bold font-mono\">\n        {{ page }}/{{ decks.length }}\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: ['decks', 'page'],\n\n  watch: {\n    page(val) {\n      this.scrollToHighlighted(val-1)\n    },\n  },\n\n  mounted() {\n    this.$nextTick(() => {\n      this.scrollToHighlighted(this.page-1, 'end')\n    })\n  },\n\n  methods: {\n    scrollToHighlighted(index, block='nearest') {\n      this.$refs.decks.children[index].scrollIntoView({\n        block\n      })\n    },\n  },\n}\n</script>\n"]}]}